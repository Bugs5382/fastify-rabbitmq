{
  "name": "fastify-rabbitmq",
  "version": "0.0.1-alpha.2",
  "description": "A Fastify RabbitMQ Plugin Developed in Pure TypeScript using the AMQPLIB.",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rm -rf dist coverage",
    "build": "tsc && tsc -p tsconfig.cjs.json && tsc -p tsconfig.types.json && ./bin/build-types.sh",
    "build:watch": "tsc -w",
    "lint": "ts-standard --parser @typescript-eslint/parser | snazzy",
    "lint:fix": "ts-standard --fix --parser @typescript-eslint/parser | snazzy",
    "prepublishOnly": "npm run build",
    "test": "jest --detectOpenHandles --forceExit",
    "test:ci": "jest --ci  --detectOpenHandles --forceExit",
    "test:coverage": "jest --coverage --detectOpenHandles  --forceExit",
    "update": "npx npm-check-updates -u && npm install && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bugs5382/fastify-rabbitmq"
  },
  "keywords": [
    "amqp-connection-manager",
    "amqplib",
    "typescript",
    "rabbitmq",
    "fastify",
    "fastify-plugin"
  ],
  "author": "Shane Froebel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bugs5382/fastify-rabbitmq/issues"
  },
  "homepage": "https://github.com/Bugs5382/fastify-rabbitmq#readme",
  "dependencies": {
    "@fastify/error": "^3.4.1",
    "@types/amqplib": "^0.10.4",
    "amqp-connection-manager": "file:../node-amqp-connection-manager",
    "amqplib": "^0.10.3",
    "fastify-plugin": "^4.5.1",
    "promise-tools": "^2.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/parser": "^6.10.0",
    "fastify": "^4.24.3",
    "jest": "^29.7.0",
    "npm-check-updates": "^16.14.6",
    "pre-commit": "^1.2.2",
    "semantic-release": "^22.0.7",
    "snazzy": "^9.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "tsd": "^0.29.0",
    "typescript": "^5.2.2"
  },
  "pre-commit": []
}
